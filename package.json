{
  "name": "nospoone-layout",
  "version": "0.0.0",
  "description": "NodeCG bundle handling nospoone's stream layout",
  "homepage": "https://github.com/nospoone/nospoone-layout",
  "author": {
    "name": "Roméo Jr. Labonté",
    "email": "romeo@q-bit.ca",
    "url": "http://twitter.com/nospoone"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg",
    "bundle",
    "layout",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~0.9.0",
    "dashboardPanels": [
      {
        "name": "first-panel",
        "title": "First Panel",
        "width": 2,
        "file": "first-panel.html",
        "headerColor": "#6441A4"
      }
    ],
    "graphics": [
      {
        "file": "follower-notify.html",
        "width": 1280,
        "height": 100,
        "singleInstance": true
      },
      {
        "file": "top-bar.html",
        "width": 1280,
        "height": 75,
        "singleInstance": true
      },
      {
        "file": "music-notify.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      }
    ],
    "soundCues": [
      {
        "name": "donation"
      },
      {
        "name": "follow"
      },
      {
        "name": "leave"
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT",
  "dependencies": {
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.8.0",
    "gulp-less": "^3.3.2",
    "gulp-pug": "^3.3.0",
    "gulp-util": "^3.0.8",
    "jquery": "^3.2.1",
    "pump": "^1.0.2"
  }
}
